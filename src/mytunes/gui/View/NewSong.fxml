<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" onKeyPressed="#onKeyPressed" prefHeight="444.0" prefWidth="432.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mytunes.gui.Controller.NewSongController">
   <children>
      <Label layoutX="168.0" layoutY="6.0" text="New / Edit song" textFill="#d74f4f">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </Label>
      <Label layoutX="60.0" layoutY="54.0" text="Title" />
      <Label layoutX="57.0" layoutY="119.0" text="Artist" />
      <Label layoutX="45.0" layoutY="184.0" text="Category" />
      <Label layoutX="58.0" layoutY="247.0" text="Time" />
      <Label layoutX="63.0" layoutY="306.0" text="File" />
      <TextField fx:id="titleOfSongField" layoutX="120.0" layoutY="49.0" />
      <TextField fx:id="songArtistField" layoutX="120.0" layoutY="114.0" />
      <ComboBox fx:id="chooseCategoryComboBox" layoutX="120.0" layoutY="179.0" prefHeight="27.0" prefWidth="167.0" />
      <TextField fx:id="songTimeField" editable="false" layoutX="120.0" layoutY="242.0" prefHeight="27.0" prefWidth="167.0" />
      <TextField fx:id="songPathField" layoutX="120.0" layoutY="301.0" />
      <Button fx:id="chooseFileButton" layoutX="318.0" layoutY="301.0" mnemonicParsing="false" onAction="#chooseFileClicked" styleClass="buttons1" stylesheets="@Main.css" text="Choose" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="newCategoryButton" layoutX="318.0" layoutY="179.0" mnemonicParsing="false" onAction="#addNewCategoryClicked" styleClass="buttons1" stylesheets="@Main.css" text="New" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="cancelNewSongButton" layoutX="320.0" layoutY="389.0" mnemonicParsing="false" onAction="#cancelNewSongClicked" styleClass="buttons2" stylesheets="@Main.css" text="Cancel" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <Button fx:id="saveSongButton" layoutX="250.0" layoutY="389.0" mnemonicParsing="false" onAction="#saveSongClicked" styleClass="buttons2" stylesheets="@Main.css" text="Save" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
